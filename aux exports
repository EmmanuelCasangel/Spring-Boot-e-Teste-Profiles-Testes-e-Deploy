export FORUM_DATABASE_URL=jdbc:h2:mem:alura-forum
export FORUM_DATABASE_USERNAME=sa
export FORUM_DATABASE_PASSSWORD=
export FORUM_JWT_SECRET=123456




java -jar -Dspring.profiles.active=prod  -DFORUM_DATABASE_URL=jdbc:h2:mem:alura-forum 
-DFORUM_DATABASE_USERNAME=sa -DFORUM_DATABASE_PASSSWORD=
-DFORUM_JWT_SECRET=123456 forum.jar


sudo docker run -p 8080:8080 -e SPRING_PROFILES_ACTIVE='prod' -e FORUM_DATABASE_URL=jdbc:h2:mem:alura-forum -e FORUM_DATABASE_USERNAME=sa -e FORUM_DATABASE_PASSSWORD= -e FORUM_JWT_SECRET=123456 curso/forum

